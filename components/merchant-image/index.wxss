.container{
    background-color:#fff;
}

.title{
    /* display:flex;
    justify-content: space-between; */
    /* height:80rpx; */
    /* align-items:center; */
    padding:20rpx;
    text-align: center;
    font-size:16px;
    border-bottom:1px solid #ddd;
    position: relative;
}
.title_right{
    position: absolute;
    right:30rpx;
    top:50%;
    transform:translateY(-50%);
    display:flex;
    justify-content: center;
    align-items:center;
}
.title_right image{
    width:40rpx;
    height:40rpx;
    margin-left:10rpx;
}

.images{
    display: flex;
    padding:20rpx;
    justify-content: space-around;
}

.images image{
    width:31%;
    height:250rpx;
}